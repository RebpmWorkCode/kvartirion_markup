[
    {
        "Region": {
            "title": "Twig | Список отзывов",
            "alias": "twig_reviews_index",
            "block_count": 1
        },
        "Block": [
            {
                "title": "Twig | Список отзывов",
                "alias": "twig_reviews_index",
                "body": "<div class=\"testimonials\">\r\n    <div class=\"container\">\r\n        <div class=\"row testimonials__top\">\r\n            <div class=\"col-md-6 offset-md-3\">\r\n                <div class=\"heading-base heading-base--medium text-center\">Отзывы о нас<\/div>\r\n            <\/div>\r\n            <div class=\"col-md-3 text-right-md\">\r\n                <a class=\"btn btn-pink\" href=\"#order_review\" data-fancybox>Оставить отзыв<\/a>\r\n            <\/div>\r\n        <\/div>\r\n\r\n        <div class=\"section-title\">{{ paginator.params.current }} отзывов <\/div>\r\n\r\n        {% set reviews = _view.get('reviews') %}\r\n        {% for review in reviews %}\r\n            <div>\r\n                <div class=\"testimonials__item\">\r\n                    <div class=\"testimonials__item-info\">\r\n                        <div class=\"testimonials__item-name\">{{ review.Review.sender }}<\/div>\r\n                        <div class=\"testimonials__item-date\">{{ review.Review.updated }}<\/div>\r\n\r\n                    <\/div>\r\n                    <div class=\"testimonials__item-text\">\r\n                        <strong>{{ review.Review.annotation }}<\/strong><br>\r\n                        {{ review.Review.text }}\r\n                    <\/div>\r\n                <\/div>\r\n            <\/div>\r\n        {% endfor %}\r\n    <\/div>\r\n<\/div>\r\n\r\n",
                "status": true,
                "params": "disabledCkEditor=1"
            }
        ]
    }
]